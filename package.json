{
  "name": "node-instagram",
  "description": "Instagram api client for node that support promises",
  "version": "1.3.2",
  "author": "Leo Pradel",
  "keywords": [
    "instagram",
    "api",
    "node"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:test": "babel test --out-dir lib-test",
    "lint": "eslint src test",
    "test": "npm run lint && npm run test:src && npm run build",
    "test-travis": "npm run lint && npm run build:test && npm run test-travis:src && npm run build",
    "test:src": "mocha --compilers js:babel-core/register ./test",
    "test-travis:src": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec ./lib-test",
    "prepublish": "publish-please guard",
    "publish-please": "publish-please"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pradel/node-instagram.git"
  },
  "bugs": {
    "url": "https://github.com/pradel/node-instagram/issues"
  },
  "homepage": "https://github.com/pradel/node-instagram#readme",
  "dependencies": {
    "lodash.isfunction": "^3.0.8",
    "request-promise": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "^8.0.0",
    "publish-please": "^2.1.4",
    "sinon": "^1.17.5"
  }
}
