{
  "name": "node-instagram",
  "description": "Instagram api client for node that support promises",
  "version": "1.5.0",
  "author": "Leo Pradel",
  "keywords": [
    "instagram",
    "api",
    "node",
    "typescript"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src test",
    "test": "npm run lint && npm run build && npm run testonly",
    "testonly": "jest --coverage"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "__tests__/utils.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "mapCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pradel/node-instagram.git"
  },
  "bugs": {
    "url": "https://github.com/pradel/node-instagram/issues"
  },
  "homepage": "https://github.com/pradel/node-instagram#readme",
  "dependencies": {
    "@types/request-promise": "^4.1.33",
    "lodash.isfunction": "^3.0.8",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0",
    "istanbul": "^0.4.5",
    "jest": "^20.0.4",
    "mocha": "^3.4.1",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "^9.0.13",
    "sinon": "^2.2.0",
    "ts-jest": "^20.0.5",
    "typescript": "^2.3.4"
  }
}
